{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-react-hooks",
        "tslint-config-prettier",
    ],
    "jsRules": {},
    "rules": {
        "ordered-imports": false,
        "trailing-comma": false,
        "no-console": false,
        "max-line-length": [
            true,
            160
        ],
        "object-literal-sort-keys": false,
        "arrow-parens": false,
        "react-hooks-nesting": "error"
    },
    "rulesDirectory": []
}
